<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <base-config cleartextTrafficPermitted="true">
        <trust-anchors>
            <certificates src="system"/>
            <certificates src="user"/> <!-- 允许用户安装的证书 -->
        </trust-anchors>
    </base-config>
</network-security-config>